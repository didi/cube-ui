{
  "name": "cube-ui",
  "version": "2.0.0-alpha.0",
  "description": "A fantastic mobile ui lib implement by Vue",
  "main": "./lib/cube.mjs",
  "module": "./src/index.js",
  "unpkg": "./lib/cube.umd.js",
  "typings": "types/index.d.ts",
  "postCompile": true,
  "scripts": {
    "build": "vite build -c vite.umd.config.ts && tsx scripts/build-components.ts",
    "dev": "vite",
    "doc-dev": "vite -c vite.doc.config.ts",
    "doc-build": "vite build -c vite.doc.config.ts",
    "doc-preview": "vite preview -c vite.doc.config.ts",
    "demo-build": "vite build",
    "doc-demo-build": "pnpm run doc-build && pnpm run demo-build && tsx scripts/build-docs.ts",
    "release": "bash ./build/release/publish.sh",
    "release-docs": "bash ./build/release/docs.sh",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "codecov": "codecov",
    "test": "npm run unit && npm run codecov",
    "cm": "git-cz"
  },
  "files": [
    "src",
    "lib",
    "types"
  ],
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:didi/cube-ui.git"
  },
  "homepage": "https://didi.github.io/cube-ui/",
  "bugs": {
    "url": "https://github.com/didi/cube-ui/issues"
  },
  "keywords": [
    "cube-ui",
    "cube",
    "vue",
    "components"
  ],
  "license": "Apache",
  "dependencies": {
    "better-scroll": "~1.12.6",
    "mitt": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/klaw-sync": "^6.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^7.1.1",
    "chalk": "^1.1.3",
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^11.1.1",
    "highlight.js": "^9.18.5",
    "husky": "^1.0.0-rc.6",
    "js-cookie": "^2.1.4",
    "klaw-sync": "^6.0.0",
    "lodash-es": "^4.17.8",
    "lolex": "^1.5.2",
    "marked": "^4.0.10",
    "opn": "^4.0.2",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "scp2": "^0.5.0",
    "shelljs": "^0.8.5",
    "stylus": "^0.59.0",
    "tsx": "^3.12.7",
    "typescript": "^4.1.6",
    "vconsole": "^2.5.2",
    "vite": "^4.3.8",
    "vite-plugin-vue-markdown": "^0.22.4",
    "vitest": "^0.29.2",
    "vue": "^3.2.45",
    "vue-lazyload": "^1.1.4",
    "vue-router": "^4.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
